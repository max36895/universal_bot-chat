{
  "name": "umchat",
  "version": "0.3.6",
  "description": "Universal chat for run Alise skills or other voice assistants on the web site",
  "keywords": [
    "chat",
    "site",
    "dialog",
    "react",
    "alisa",
    "typescript",
    "bot"
  ],
  "author": "Maxim-M",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "shx": "^0.3.2",
    "rollup": "^3.10.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "18.0.0"
  },
  "scripts": {
    "rollup": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup --config rollup.config.dev.js --bundleConfigAsCjs",
    "build": "shx rm -rf dist && npm run rollup && npm publish"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/max36895/universal_bot-chat/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/max36895/universal_bot-chat.git"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
